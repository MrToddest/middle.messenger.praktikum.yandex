    "@types/node": "17.0.42",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "acorn": "8.7.1",
    "chai": "4.3.6",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "8.17.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "express": "4.18.1",
