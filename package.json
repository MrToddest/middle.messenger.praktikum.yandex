    "pug-loader": "2.4.0",
    "sass": "1.52.3",
    "sass-loader": "13.0.0",
    "sinon": "14.0.0",
    "stylelint": "14.9.1",
    "stylelint-config-standard-scss": "4.0.0",
    "svg-sprite-loader": "6.0.11",
    "ts-loader": "9.3.0",
    "ts-node": "10.8.1",
